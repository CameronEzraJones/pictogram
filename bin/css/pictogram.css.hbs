.pictogram {
  width:40px;
  height:40px;
  background: rgba(200,200,200,0.1) no-repeat;
  background-size: 100%;
  box-shadow: inset 0px 1px 4px 1px rgba(0,0,0,0.1);
}
.pictogram-lg {
  width:60px;
  height:60px;
}
{{#each this}}
.pictogram-{{name}} {
  background-image: url({{url}});
  background-color: transparent;
  box-shadow:0 0;
}
{{/each}}
